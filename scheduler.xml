<?xml version="1.0" encoding="utf-8"?>
<openerp>
   <data noupdate="1">
<!-- Scheduler for Clean data-->	
		<record forcecreate="True" id="ir_cron_import_ingram"  model="ir.cron">
			<field name="name">Ingram Synchronization</field>
			<field eval="False" name="active" />
			<field name="user_id" ref="base.user_root" />
			<field name="interval_number">1</field>
			<field name="interval_type">days</field>
			<field name="numbercall">-1</field>
			<field eval="False" name="doall" />
			<field eval="'ingram_config'" name="model" />
			<field eval="'cron_function'" name="function" />                                                                            
			<field eval="'(False,)'" name="args" />
		</record>
   </data>
</openerp>
